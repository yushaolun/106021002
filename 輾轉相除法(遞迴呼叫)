import java.util.Scanner;

public class c8 {
    public static void main(String[]args){
        Scanner sc=new Scanner(System.in);
        int a=sc.nextInt();
        int b=sc.nextInt();
        System.out.println(fu(a,b));
    }
public static int fu(int a,int b){
     if (b==0){
         return a;
     }
     else {
         return fu(b,a%b);
     }





}









}
