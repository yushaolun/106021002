import java.util.Scanner;

public class b14 {
    public static void main(String[]args){
        Scanner sc=new Scanner(System.in);
        int a=sc.nextInt();
        for (int i=1;a>=i;i++){
            for (int j=1;i>=j;j++){
                System.out.print("*");
            }
            System.out.println();
        }











    }











}
