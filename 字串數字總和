import java.util.Scanner;

public class b4 {
    public static void main(String[]args){
        Scanner sc=new Scanner(System.in);
        String a=sc.nextLine();
        char []b=a.toCharArray();
        int c=0;
        for (int i=0;b.length>i;i++){
            c=(int)b[i]+c-48;
        }
        System.out.println(c);







    }












}
