import java.util.Scanner;

public class b1 {
    public static void main(String[]args){
        Scanner sc=new Scanner(System.in);
        int a=sc.nextInt();
        int b=sc.nextInt();
        float kg=0;
        if (b==1){
            System.out.println(Math.round(10)*(a-80)*0.7/10);
        }
        if (b==2){
            System.out.println(Math.round(10)*(a-70)*0.6/10);
        }



    }








}
